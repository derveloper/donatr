{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-inferno-app": "^1.5.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "5.0.1",
    "css-loader": "0.26.1",
    "detect-port": "1.1.0",
    "dotenv": "4.0.0",
    "eslint": "3.14.1",
    "eslint-config-inferno-app": "^0.6.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-inferno": "6.9.2",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "filesize": "3.5.4",
    "fs-extra": "2.0.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.1",
    "http-proxy-middleware": "0.17.3",
    "inferno-dev-utils": "^0.7.0",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.1",
    "path-exists": "3.0.0",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "babel-plugin-inferno": "^1.7.0",
    "history": "^4.5.1",
    "inferno": "^1.2.2",
    "inferno-compat": "^1.2.2",
    "inferno-component": "^1.2.2",
    "inferno-redux": "^1.2.2",
    "inferno-router": "^1.2.2",
    "md5": "^2.2.1",
    "react-jss": "^5.3.0",
    "redux-saga": "^0.14.3",
    "sweetalert": "^1.1.3",
    "whatwg-fetch": "^2.0.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js --optimize-minimize",
    "test": "node scripts/test.js --env=jsdom"
  },
  "proxy": "http://localhost:8080",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "inferno-app"
    ]
  },
  "eslintConfig": {
    "extends": "inferno-app"
  }
}
